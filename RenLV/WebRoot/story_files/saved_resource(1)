function setmarkermaxzindex(k){markers[k].setZIndex(google.maps.Marker.MAX_ZINDEX);}
function setmarkersmallicon(){$.each(markers,function(k,v){if(k!==infowindowshow){v.setIcon(seticon(k,"small"));}})}
function setinfowindow(k){infowindowshow=k;doing.getDoings(markerarray[k].id,type,0,function(doingdata){contentString[k]='<div>'+'<div class="pla_mapfloat_poi clearfix">'+'<p class="pic"><a href="'+markerarray[k].url+'" target="_blank"><img src="'+markerarray[k].imgurl+'" width="80" height="80" alt="" /></a></p>'+'<p class="titcn"><a href="'+markerarray[k].url+'" target="_blank">'+(markerarray[k].chinesename?markerarray[k].chinesename:"")+'</a></p>'+'<p class="titen"><a href="'+markerarray[k].url+'" target="_blank">'+(markerarray[k].englishname?markerarray[k].englishname:"")+'</a></p>';contentString[k]+='<p class="yelps">'+'<span class="stars">'+markerarray[k].star+'</span>'+'<span class="sum"><a href="'+markerarray[k].commenturl+'" target="_blank">'+markerarray[k].commentcounts+'æ¡ç¹è¯</a></span>'+'</p>';contentString[k]+='<p class="yelps"></p>';contentString[k]+='<p class="stat">'+markerarray[k].doings.counts.beentocounts+'äººå»è¿</p>'+'</div>'+'<ul class="pla_mapfloat_poibtm">';contentString[k]+='</ul>'+'</div>';infowindow[k]=new google.maps.InfoWindow({content:contentString[k],disableAutoPan:false});infowindow[k].open(map,markers[k]);google.maps.event.addListener(infowindow[k],'closeclick',function(){infowindowshow=false;markers[k].setIcon(seticon(k,"small"));});});}
function mapinit(){LatLng=[];markers=[];infowindow=[];infowindowshow=false;contentString=[];if(markerarray.length>0){if(markerarray.length==1){if(typeof nowbounds=="undefined"){setmapcenter(markerarray[0].lat,markerarray[0].lng);setmapzoom(13);}
LatLng[0]=setmaplatlng(markerarray[0].lat,markerarray[0].lng);markers[0]=setmapmarker(LatLng[0],seticon(0,"small"),markerarray[0]);setmarkerevent(markers[0],0);}else{bounds=new google.maps.LatLngBounds();$.each(markerarray,function(k,v){LatLng[k]=setmaplatlng(v.lat,v.lng);bounds.extend(LatLng[k]);markers[k]=setmapmarker(LatLng[k],seticon(k,"small"),v);setmarkerevent(markers[k],k);});map.fitBounds(bounds);}}}
function mguide_clearmarker(){$.each(markers,function(k,v){v.setMap(null);})
markers.length=0;}
