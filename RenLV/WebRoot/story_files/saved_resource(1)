function seticon(k,size){if(isNaN(parseInt(k))){k=0;}
var height=(parseInt(k)+1)*50;if(size=="small"){icon=new google.maps.MarkerImage("http://static.qyer.com/images/place5/map_pin.png",new google.maps.Size(27,31),new google.maps.Point(50,height));}else{icon=new google.maps.MarkerImage("http://static.qyer.com/images/place5/map_pin.png",new google.maps.Size(37,43),new google.maps.Point(0,height));}
return icon;}
